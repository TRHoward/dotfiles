filetype plugin indent on
syntax on
set relativenumber
set number
set ignorecase
set smartcase
set history=100
set undolevels=100
set tabstop=4
set shiftwidth=4
set expandtab
set noerrorbells
set nowrap
set noswapfile
set autoindent
set nolist
set novb
set nobackup
set nowritebackup
set noswapfile
set incsearch
nnoremap <Space> <Nop>
let mapleader = " "
nnoremap <leader>py "0p
nnoremap <leader>nt :Te<CR>
nnoremap <leader>h :sp<CR>
nnoremap <leader>v :vsp<CR>
nnoremap <leader>pc "*p
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
:command WQ wq
:command W w
:command Q q
:command Wq wq
